{
  "name": "invest_core",
  "version": "0.0.1",
  "description": "",
  "author": "",
  "private": true,
  "license": "UNLICENSED",
  "scripts": {
    "prebuild": "rimraf dist",
    "build": "nest build && nest build proto-schema",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "start": "nest start",
    "start:dev": "nest start --watch",
    "start:debug": "nest start --debug --watch",
    "start:prod": "node dist/main",
    "lint": "eslint \"{src,apps,libs,test,db}/**/*.ts\"",
    "lint-fix": "eslint \"{src,apps,libs,test,db}/**/*.ts\" --fix",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "jest --config ./test/jest-e2e.json --detectOpenHandles --forceExit",
    "typeorm": "./node_modules/.bin/ts-node -r tsconfig-paths/register ./node_modules/typeorm/cli.js",
    "migration:up": "yarn typeorm migration:run -d ./db/data-source.ts",
    "migration:down": "yarn typeorm migration:revert -d ./db/data-source.ts"
  },
  "dependencies": {
    "@faker-js/faker": "^9.4.0",
    "@grpc/grpc-js": "^1.5.5",
    "@grpc/proto-loader": "^0.7.3",
    "@nestjs-query/core": "^0.30.0",
    "@nestjs/axios": "^3.0.2",
    "@nestjs/common": "^10.3.7",
    "@nestjs/config": "^3.2.2",
    "@nestjs/core": "^10.3.7",
    "@nestjs/cqrs": "^10.2.7",
    "@nestjs/microservices": "^10.3.7",
    "@nestjs/platform-express": "^10.3.7",
    "@nestjs/platform-fastify": "^10.3.7",
    "@nestjs/schedule": "^4.0.1",
    "@nestjs/typeorm": "^10.0.2",
    "amqp-connection-manager": "^4.1.3",
    "amqplib": "^0.10.0",
    "axios": "^1.6.8",
    "bcrypt": "^5.1.1",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.13.2",
    "dataloader": "^2.0.0",
    "express": "^4.17.3",
    "geojson": "^0.5.0",
    "jsona": "^1.9.7",
    "lodash": "^4.17.21",
    "pg": "^8.7.3",
    "pluralize": "^8.0.0",
    "reflect-metadata": "^0.1.13",
    "rimraf": "^3.0.2",
    "rxjs": "^7.2.0",
    "typeorm": "^0.3.17",
    "typeorm-transactional-tests": "^2.0.0"
  },
  "devDependencies": {
    "@golevelup/ts-jest": "^0.3.3",
    "@nestjs/cli": "^10.3.2",
    "@nestjs/schematics": "^10.1.1",
    "@nestjs/testing": "^10.3.7",
    "@types/amqplib": "^0.10.4",
    "@types/bcrypt": "^5.0.2",
    "@types/express": "^4.17.13",
    "@types/geojson": "^7946.0.8",
    "@types/jest": "^29.5.12",
    "@types/lodash": "^4.14.182",
    "@types/node": "^18.0.6",
    "@types/supertest": "^2.0.11",
    "@typescript-eslint/eslint-plugin": "^5.23.0",
    "@typescript-eslint/parser": "^5.0.0",
    "eslint": "^8.7.0",
    "eslint-config-airbnb-base": "^15.0.0",
    "eslint-config-airbnb-typescript": "^17.0.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-import": "^2.25.4",
    "eslint-plugin-no-only-tests": "^3.1.0",
    "eslint-plugin-prettier": "^4.0.0",
    "eslint-plugin-simple-import-sort": "^12.0.0",
    "fishery": "^2.2.2",
    "jest": "^29.7.0",
    "jest-extended": "^4.0.2",
    "prettier": "^2.3.2",
    "source-map-support": "^0.5.20",
    "supertest": "^6.1.3",
    "ts-jest": "^29.1.2",
    "ts-loader": "^9.2.3",
    "ts-node": "^10.0.0",
    "ts-proto": "^1.178.0",
    "ts-protoc-gen": "^0.15.0",
    "tsconfig-paths": "^4.0.0",
    "typescript": "^5.4.4"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": ".",
    "roots": [
      "<rootDir>/src/",
      "<rootDir>/libs/"
    ],
    "modulePaths": [
      "<rootDir>"
    ],
    "moduleNameMapper": {
      "^@proto-schema(|/.*)$": "<rootDir>/libs/proto-schema/gen/$1"
    },
    "testRegex": ".*\\.spec\\.ts$",
    "testPathIgnorePatterns": [
      ".factory.spec.ts$"
    ],
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "collectCoverageFrom": [
      "**/*.(t|j)s"
    ],
    "coverageDirectory": "../coverage",
    "testEnvironment": "node",
    "setupFilesAfterEnv": [
      "jest-extended/all"
    ]
  },
  "engines": {
    "npm": ">=10.5.0",
    "node": ">=20.0.0 <21.0.0"
  }
}
